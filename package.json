    "dev": "vite",
    "build": "vite build",
    "build:odr": "vite build --config vite.odr.config.ts",
    "serve": "serve -s dist",
    "tunnel": "vk-tunnel --insecure=1 --http-protocol=https --ws-protocol=wss --host=0.0.0.0 --port=10888",
    "build:serve:tunnel": "yarn build && concurrently \"yarn serve -p 10888\" \"yarn tunnel\"",
    "dev:tunnel": "concurrently \"yarn dev --config vite.tunnel.config.ts\" \"yarn tunnel --port=10888\"",
    "deploy": "vk-miniapps-deploy",
    "preview": "vite preview --port 8080",
    "cypress:open": "cypress open",
    "eslint": "eslint --format=table --fix ."
